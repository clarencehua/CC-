
#include <stdio.h>

int main()
{
	// 以下几种写法完全等价
	printf("Got: %c \n", 65 );
	printf("Got: %c \n", 'A');
	printf("Got: %c \n", 0x41);


	// 打印'Y'对应的 ASCII码 
	printf ("%d  \n" , 'Y' );   

	// 打印  "1+2"
	printf("%c%c%c \n",  '1', '+', '2' );

	// 换行显示
	printf("%c%c%c%c", '1', '\n', '2', '\n');

	char ch = '\t';

	char str[6] = "hello";
	printf("string: %s \n", str);
	return 0;
}

