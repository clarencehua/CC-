#include <stdio.h>

class Circle
{
public:
   Circle ()
   {
      x = y = 0;
      radius = 1;
   }
   Circle(int x, int y, int r)
   {
       this->x = x;
       this->y = y;
       this->radius = r;
   }
public:
   int x, y;
   int radius;
};


int main()
{
	Circle c1; // 定义一个对象, 并使用它
	printf("location:(%d,%d), radius:%d \n", c1.x, c1.y, c1.radius);
	return 0;
}



