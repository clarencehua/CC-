#include <stdio.h>

class Circle
{
public:
   Circle ()
   {
      x = y = 0;
      radius = 1;
	  printf("构造函数(1) \n");
   }
   Circle(int x, int y, int r)
   {
       this->x = x;
       this->y = y;
       this->radius = r;
	   printf("构造函数(2) \n");
   }
public:
   int x, y;
   int radius;
};


int main()
{
	Circle  a;
	Circle  b(1,1, 4);  
	return 0;
}



